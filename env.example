# Cloudflare R2 設定
R2_ENDPOINT_URL=https://your-account-id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your_access_key_id
R2_SECRET_ACCESS_KEY=your_secret_access_key
R2_BUCKET_NAME=analyzeap-data
R2_PRODUCT_MASTER_KEY=product_master.csv

# Flask設定
SECRET_KEY=your-secret-key-change-in-production

# GA4 API設定（オプション）
# サービスアカウントのJSONを1行に圧縮して設定
GA4_CREDENTIALS_JSON={"type":"service_account","project_id":"...","private_key":"..."}

# 各ブランドのGA4プロパティID
GA4_PROPERTY_RADY=123456789
GA4_PROPERTY_CHERIMI=123456789
GA4_PROPERTY_MICHELLMACARON=123456789
GA4_PROPERTY_RADYCHARM=123456789
